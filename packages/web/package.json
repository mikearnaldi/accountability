{
  "name": "@accountability/web",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "sideEffects": false,
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "generate": "pnpm generate:routes && pnpm generate:api",
    "generate:routes": "npx @tanstack/router-cli generate",
    "generate:api": "tsx scripts/generate-api-client.ts",
    "preview": "vite preview",
    "start": "node .output/server/index.mjs",
    "clean": "rm -rf dist .output",
    "test": "cd ../.. && vitest run packages/web/test",
    "test:e2e": "playwright test",
    "test:e2e:verbose": "VERBOSE=1 playwright test",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:report": "playwright show-report",
    "test:e2e:synthetic": "playwright test synthetic-data-generator.spec.ts",
    "generate:synthetic-data": "tsx scripts/generate-synthetic-data.ts"
  },
  "dependencies": {
    "@accountability/api": "workspace:*",
    "@floating-ui/react": "^0.27.16",
    "@tailwindcss/vite": "^4.1.18",
    "@tanstack/react-router": "^1.147.0",
    "@tanstack/react-router-devtools": "^1.147.0",
    "@tanstack/react-start": "^1.147.0",
    "clsx": "^2.1.1",
    "jspdf": "^4.0.0",
    "jspdf-autotable": "^5.0.7",
    "lucide-react": "^0.562.0",
    "openapi-fetch": "^0.13.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "scheduler": "^0.27.0",
    "tailwindcss": "^4.1.18",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@effect/platform": "^0.94.0",
    "@effect/sql": "^0.49.0",
    "@effect/sql-pg": "^0.50.1",
    "@playwright/test": "^1.57.0",
    "@testcontainers/postgresql": "^10.14.0",
    "@types/node": "^22.5.4",
    "@types/react": "^19.0.8",
    "@types/react-dom": "^19.0.3",
    "@vitejs/plugin-react": "^5.1.2",
    "effect": "^3.19.0",
    "nitro": "npm:nitro-nightly@latest",
    "openapi-typescript": "^7.5.0",
    "tsx": "^4.21.0",
    "typescript": "^5.7.2",
    "vite": "^7.1.7",
    "vite-tsconfig-paths": "^5.1.4"
  }
}
